{
  "name": "@dom-native/ui",
  "version": "0.1.3",
  "description": "Minimalist, stylable, and extensible DOM UI base components based dom-native",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/ui.git"
  },
  "keywords": [
    "web-component",
    "native",
    "ui components",
    "html5",
    "DOM",
    "DOM Centric",
    "Patterns",
    "html",
    "framework",
    "web-framework",
    "drag and drop"
  ],
  "dependencies": {
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@dom-native/demo-infra": "0.0.0-internal-2",
    "cmdrouter": "^0.3.1",
    "dom-native": "^0.9.9",
    "handlebars": "^4.7.6",
    "sketchdev": "^0.5.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "vdev": "^0.12.1"
  },
  "peerDependencies": {
    "dom-native": "^0.9.9"
  },
  "files": [
    "src/",
    "dist/",
    "pcss/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "code": "npm run watch & npm run start",
    "sketch": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts sketch",
    "build": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts build",
    "watch": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts watch",
    "site": "./node_modules/.bin/vdev sup demo/ jc-sites:dom-native/demo/ui/"
  },
  "engines": {
    "node": ">=8.2.0"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT"
}
